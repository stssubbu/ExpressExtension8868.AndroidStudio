<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_focused="true" android:state_pressed="true">
        <shape>
            <stroke android:width="2.2999878dip" android:color="#fff2f0ee" />
            <corners android:radius="10.0dip" />
        </shape>
    </item>
    <item android:state_focused="false" android:state_pressed="true">
        <shape>
            <stroke android:width="2.2999878dip" android:color="#fff2f0ee" />
            <corners android:radius="10.0dip" />
        </shape>
    </item>
    <item android:state_focused="true" android:state_pressed="false">
        <shape>
            <stroke android:width="0.70000005dip" android:color="#ff000000" />
            <corners android:radius="10.0dip" />
        </shape>
    </item>
    <item android:state_focused="false" android:state_pressed="false">
        <shape>
            <gradient android:startColor="#fff2f0ee" android:endColor="#fff2f0ee" android:angle="270.0" android:centerColor="#fff2f0ee" />
            <stroke android:width="0.70000005dip" android:color="#ff000000" />
            <corners android:radius="10.0dip" />
        </shape>
    </item>
    <item android:state_enabled="true">
        <shape>
            <padding android:left="4.0dip" android:top="4.0dip" android:right="4.0dip" android:bottom="4.0dip" />
        </shape>
    </item>
</selector>